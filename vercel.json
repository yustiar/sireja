{
    "builds": [{
        "src": "sireja/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }

    }],

    "routes":[
        {"src": "/(.*)", "dest": "sireja/wsgi.py"},
        { "src": "/static/(.*)", "dest": "/static/$1" },
        { "src": "/(.*)", "dest": "manage.py" }

    ]
}